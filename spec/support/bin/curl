#!/usr/bin/env node

var spawn = require('child_process').spawn;
var mrt = require('../../acceptance/helpers');
var _ = require('underscore');

var doCurl = function(uname, arch) {
  var curl = spawn('/usr/bin/curl', ['-#', 'http://localhost:7357/dev_bundle_' + uname + '_' + arch + '_0.1.5.tar.gz'], function() {
    
  });
  curl.stderr.pipe(process.stderr);
  curl.stdout.pipe(process.stdout);
};

mrt.getSystemInfo(doCurl);
